<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="WToolbar">
        <attr name="title" format="string" />
        <attr name="titleTextColor" format="color" />
        <attr name="titleTextAppearance" format="reference" />
        <attr name="subtitle" format="string" />
        <attr name="subtitleTextColor" format="color" />
        <attr name="subtitleTextAppearance" format="reference" />
        <attr name="navigationIcon" format="reference" />
        <attr name="navigationContentDescription" format="string" />
    </declare-styleable>

    <attr name="wToolbarStyle" format="reference" />

    <declare-styleable name="EmptyView">
        <attr name="message" format="string" />
    </declare-styleable>

    <declare-styleable name="SmoothCheckBox">
        <attr name="duration" format="integer"/>
        <attr name="stroke_width" format="dimension"/>
        <attr name="color_tick" format="color"/>
        <attr name="color_checked" format="color"/>
        <attr name="color_unchecked" format="color"/>
        <attr name="color_unchecked_stroke" format="color"/>
    </declare-styleable>

    <declare-styleable name="TimeLine">
        <attr name="max_count" format="integer" />
        <attr name="line_color" format="color" />
        <attr name="text_color" format="color" />
        <attr name="text_size" format="dimension" />
    </declare-styleable>

</resources>